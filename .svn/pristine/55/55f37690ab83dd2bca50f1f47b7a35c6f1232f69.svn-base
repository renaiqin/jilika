* {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

ul,
li {
  list-style: none;
}

a {
  text-decoration: none;
}

html,
body {
  width: 100%;
  height: 100%;
  font-family: "Helvetica Neue", Helvetica, Arial, "PingFang SC", "Hiragino Sans GB", "Heiti SC", "Microsoft YaHei", "WenQuanYi Micro Hei";
  background-color: #fff;
  font-size: 36px;
}

.mask{width:100%; height:100%;top:0;left:0;position:fixed;z-index:500;background:rgba(0, 0, 0, .7);}
.maskTm{width:100%; height:100%;top:0;left:0;position:fixed;z-index:500;background:#FFF;filter:alpha(opacity=0);-moz-opacity:0;opacity: 0;}
.imask{width:100%; height:100%;top:0;left:0;position:absolute;z-index:100;background:#000;filter:alpha(opacity=70);-moz-opacity:0.7;opacity: 0.7;}
.clear:after{content:"\20";display:block;height:0;clear:both}

.ak{
  position: relative;
  -webkit-overflow-scrolling: touch;
  z-index:1;
}

