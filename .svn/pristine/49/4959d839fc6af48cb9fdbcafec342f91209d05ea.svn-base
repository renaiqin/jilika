const selteacher = {
    state:{
        department:[],
        allTeacher:[],
        choiceDepartment:{},
        choiceTeacher:{},
        cardType:[],
        cardSubType:[],
    },
    mutations:{
        SET_DEPARTMENT:(state,data) => {
            state.department = data;
        },
        SET_ALLTEACHER:(state,data) => {
            state.allTeacher = data;
        },
        SET_CHOICEDEPARTMENT:(state,data) => {
            state.choiceDepartment = data;
        },
        SET_CHOICETEACHER:(state,data) => {
            state.choiceTeacher = data;
        },
        SET_CARDTYPE:(state,data) => {
            state.cardType = data;
        },
        SET_CARDSUBTYPE:(state,data) => {
            state.cardSubType = data;
        },
        
    },
    actions:{
        setDepartment({commit},data){
            commit("SET_DEPARTMENT",data);
        },
        setAllTeacher({commit},data){
            commit("SET_ALLTEACHER",data);
        },
        setChoiceDepartment({commit},data){
            commit("SET_CHOICEDEPARTMENT",data);
        },
        setChoiceTeacher({commit},data){
            commit("SET_CHOICETEACHER",data);
        },
        setCardType({commit},data){
            commit("SET_CARDTYPE",data);
        },
        setCardSubType({commit},data){
            commit("SET_CARDSUBTYPE",data);
        }
    }
}

export default selteacher